<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ZipCodeSearch"> 
	<select id="addrSearch" parameterType="String" resultType="kr.co.toplac.util.etc.ZipCodeVO">
		SELECT zipcode, addr1
		  FROM zipcode
	     WHERE addr1 like CONCAT('%',#{searchStr},'%')
	</select>
</mapper>
